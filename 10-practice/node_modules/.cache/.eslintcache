  />\n        {emailInputIsInValid && (\n          <p className=\"error-text\">입력칸에 email을 입력해주세요!</p>\n        )}\n      </div>\n      <div className=\"form-actions\">\n        <button disabled={!formIsValid}>Submit</button>\n      </div>\n    </form>\n  );\n};\n\nexport default SimpleInput;\n",{"ruleId":"19","severity":1,"message":"20","line":1,"column":10,"nodeType":"21","messageId":"22","endLine":1,"endColumn":19},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar"]